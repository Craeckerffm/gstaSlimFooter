{
  "name": "gsta/slim-footer",
  "description": "A sleek minimal footer for Shopware 6",
  "type": "shopware-platform-plugin",
  "license": "MIT",
  "version": "1.1.0",
  "authors": [{
    "name": "Gregor Staab",
    "email": "webmaster@webgestaltung.net"
  }],
  "autoload": {
    "psr-4": {
      "gstaSlimFooter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "gstaSlimFooter\\Test\\": "tests/"
    }
  },
  "require": {
    "shopware/core": ">=6.1",
    "shopware/storefront": ">=6.1"
  },
  "require-dev": {
      "phpunit/phpunit": "~8.3.4",
      "phpstan/phpstan": "^0.12.18"
},
  "extra": {
    "shopware-plugin-class": "gstaSlimFooter\\gstaSlimFooter",
    "copyright": "(c) by Gregor Staab",
    "plugin-icon": "src/Resources/config/plugin.png",
    "label": {
      "de-DE": "Minimale Fussleiste",
      "en-GB": "Minimal Footer"
    }
  }
}
